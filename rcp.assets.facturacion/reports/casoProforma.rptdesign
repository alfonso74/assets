            <property name="dataSource">Assets Produccion</property>
            <list-property name="resultSet">
                <structure>
                    <property name="position">1</property>
                    <property name="name">descripcion</property>
                    <property name="nativeName">descripcion</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">2</property>
                    <property name="name">desc_ingles</property>
                    <property name="nativeName">desc_ingles</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">3</property>
                    <property name="name">prioridad</property>
                    <property name="nativeName">prioridad</property>
                    <property name="dataType">integer</property>
                </structure>
                <structure>
                    <property name="position">4</property>
                    <property name="name">totalCargo</property>
                    <property name="nativeName">totalCargo</property>
                    <property name="dataType">decimal</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[select * from (
(SELECT tc.descripcion, tc.desc_ingles, tc.prioridad, sum(d.monto) as totalCargo FROM det_cargos d
left outer join tipo_cargos tc on tc.no_tipo_cargo = d.no_tipo_cargo
inner join cargos c on c.idCargo = d.idCargo
inner join casos caso on caso.idCaso = c.idCaso
where caso.idCaso = ? and d.idGrupo is null
group by descripcion, desc_ingles, prioridad)
union
(SELECT g.descripcion, g.descripcion, g.grupo, sum(d.monto) as totalCargo
FROM det_cargos d
left outer join tipo_cargos tc on tc.no_tipo_cargo = d.no_tipo_cargo
inner join grupo_cargos g on g.idGrupo = d.idGrupo
inner join cargos c on c.idCargo = d.idCargo
inner join casos caso on caso.idCaso = c.idCaso
where caso.idCaso = ? and d.idGrupo is not null
group by g.descripcion, g.descripcion, g.grupo)
) detalles
where totalCargo > 0
order by prioridad, descripcion]]></xml-property>
        </oda-data-set>
