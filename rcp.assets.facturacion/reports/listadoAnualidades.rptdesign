            <property name="dataSource">expedientes</property>
            <method name="open"><![CDATA[Packages.java.lang.System.out.println("---------- Exportando anualidades a excel -------------");

anualidades = reportContext.getParameterValue("listadoExpedientes");
Packages.java.lang.System.out.println("Anualidades: " + anualidades);

count = 0;]]></method>
            <method name="fetch"><![CDATA[if (anualidades != null &amp;&amp; count < anualidades.size()) {
	row["noExp"] = anualidades.get(count).getNoExpediente();
	row["nombre"] = anualidades.get(count).getNombre();
	row["tasa"] = anualidades.get(count).getCargoExpediente().getCargosAnuales().getTasa();
	row["totalAnualidad"] = anualidades.get(count).getCargoExpediente().getTotalCargosAnuales();
	count++;
	return true;
}
return false;]]></method>
